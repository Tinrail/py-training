iptables -A INPUT -p tcp -m multiport --dports 80,443,25 -j ACCEPT
iptables -A INPUT -m state --state ESTABLISHED,RELATED,NEW -j ACCEPT
iptables -A INPUT -i lo -j ACCEPT
iptables -A INPUT -s 10.0.168.0/24 -p tcp --dport 22 -j ACCEPT
iptables -A INPUT -s 10.0.34.9 -p tcp --dport 22 -j ACCEPT
iptables -A INPUT -s 10.0.34.10 -p tcp --dport 22 -j ACCEPT
iptables -A INPUT -j DROP
iptables-save > /etc/sysconfig/iptables
exit


